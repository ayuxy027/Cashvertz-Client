{
  "name": "cashvertz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "npm run typecheck && npm run lint && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && rm -rf dist && npm cache clean --force",
    "fresh": "npm run clean && npm install",
    "ci": "npm run typecheck && npm run lint && npm run build"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/vite": "^4.1.13",
    "@vercel/analytics": "^1.5.0",
    "dotenv": "^17.2.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5"
  }
}
